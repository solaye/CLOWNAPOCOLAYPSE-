<div>
	<a class="nav-button btn btn-primary" href="/" role="button">Home</a>
	<h1>Add new iteM:</h1>
</div>
<form id="addContact">
	<div class="form-group">
		<!-- Item Name -->
		<label for="firstName"> Name</label>
		<input type="text" class="form-control" id="nameClown" name="firstName" placeholder="Name">
	</div>

	<div class="form-group">
		<!-- Text -->
		<label for="lastName">Text</label>
		<input type="text" class="form-control" id="textClown" name="lastName" placeholder="Text">
	</div>

	<div class="form-group">
		<!-- Category -->
		<label for="contactType">Category</label>
		<select id="categoryClown" class="form-control" name="contactType">
			<option value="Gear">Gear</option>
			<option value="Food">Food</option>
            <option value="Weapons">Weapon</option>
		</select>
	</div>

	<div class="form-group">
		<!-- Quantity -->
		<label for="phoneNumber">Quantity</label>
		<input type="text" class="form-control" id="quantityClown" name="phoneNumber" placeholder="Quantity">
	</div>

	<div class="form-group">
		<!-- Price -->
		<label for="emailAddress">Price</label>
		<input type="text" class="form-control" id="priceClown" name="emailAddress" placeholder="Price">
	</div>

	<input type="submit" value="Submit" id="submit" class="btn btn-primary">
</form>